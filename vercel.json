{
  "framework": "nextjs",
  "buildCommand": "npm run build:vercel",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "maorz.co.il"
        },
        {
          "type": "protocol",
          "value": "http"
        }
      ],
      "destination": "https://maorz.co.il/:1",
      "permanent": true
    }
  ]
}
